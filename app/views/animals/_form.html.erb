<%= form_for animal do |f| %>

    <%= f.label :nome %>
    <%= f.text_field :nome %>

    <%= f.label :especie %>
    <%= f.text_field :especie %>

    <%= f.label :sexo %>
    <%= f.text_field :sexo %>

    <%= f.label :cidade %>
    <%= f.text_field :cidade %>

    <%= f.label :bairro %>
    <%= f.text_field :bairro %>

    <%= f.submit %>


<% end %>